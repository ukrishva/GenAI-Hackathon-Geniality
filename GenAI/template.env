# General project configuration
# Unique project identifier
PROJECT_ID=yourprojectid
# Cloud region for project deployment
LOCATION=asia-southeast1

# Firebase and logging configurations
# Path to Firebase credentials JSON file
FIREBASE_CRED_PATH=service-account/your-firebase-service-account.json
# Firestore collection name for advertisements
FIREBASE_COLLECTION=yourfirebasecollection
# Path to log file for application logging
LOG_FILE_PATH=yourlogfilepath

# Model configuration
# Identifier for image generation model
IMAGEN_MODEL=imagegeneration@006
# Identifier for gemini model
GEMINI_MODEL=gemini-1.5-pro-preview-0409

# Path to product data template
PRODUCT_FILE_PATH=data/promotion_product_template.xlsx
# Storage bucket name
BUCKET_NAME=yourbucketname
# Number of images to generate per one variation 
N_IMAGES=4

# Product variables
# Weather conditions to target
WEATHER_CONDITIONS=SUNNY,RAINY,CLOUDY
# Geographical region to target
REGIONS=CENTER,NORTH_EAST,NORTH
# Target audience gender
GENDERS=FEMALE,MALE
# Target age groups
AGE_GROUPS=ADULT,YOUTH,ELDER
# Type of job categories to target
JOB_TYPES=WHITE_COLLAR,BLUE_COLLAR
# Age Range 
AGE_RANGE_YOUTH=10-15
AGE_RANGE_ADULT=25-40
AGE_RANGE_ELDER=45-65